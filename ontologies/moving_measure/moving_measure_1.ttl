@prefix : <http://www.alessandrolucco.it/ontologies/moving_measure/> .
@prefix dbo: <https://www.dbpedia.org/resources/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix moving_measure: <http://www.alessandrolucco.it/ontologies/moving_measure#> .
@base <http://www.alessandrolucco.it/ontologies/moving_measure/> .

<http://www.alessandrolucco.it/ontologies/moving_measure> rdf:type owl:Ontology ;
                                                           owl:versionIRI <http://www.alessandrolucco.it/ontologies/moving_measure/1.0.0> ;
                                                           rdfs:comment "Ontologia per descrivere il dominio legato alle misure in movimento" ;
                                                           rdfs:label "Misure in movimento" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#category
prov:category rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#component
prov:component rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#constraints
prov:constraints rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#definition
prov:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#dm
prov:dm rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#editorialNote
prov:editorialNote rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#editorsDefinition
prov:editorsDefinition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#inverse
prov:inverse rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#n
prov:n rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#qualifiedForm
prov:qualifiedForm rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/prov#sharesDefinitionWith
prov:sharesDefinitionWith rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.alessandrolucco.it/ontologies/moving_measure#collect
moving_measure:collect rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf prov:wasInfluencedBy ;
                       rdfs:domain moving_measure:MeasurementAct ;
                       rdfs:range moving_measure:MeasuredData ;
                       rdfs:comment """Una proprietà più specifica per indicare il tipo di dati raccolti.
Ogni attività di misura deve avere almeno un tipo di dati raccolti"""@it .


###  http://www.alessandrolucco.it/ontologies/moving_measure#employeeIn
moving_measure:employeeIn rdf:type owl:ObjectProperty ;
                          rdfs:domain prov:Person ;
                          rdfs:range prov:Organization ;
                          rdfs:comment "Identifica la relazione tra una persona e l'organizzazione di cui fa parte" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#followGPSPath
moving_measure:followGPSPath rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf prov:wasDerivedFrom ;
                             rdfs:domain moving_measure:Measure ;
                             rdfs:range moving_measure:PathGPS ;
                             rdfs:comment "Serve per segnalare se la misura è stata fatta a partire da un pergorso GPS già deciso in precedenza" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#haveDataAbout
moving_measure:haveDataAbout rdf:type owl:ObjectProperty ;
                             rdfs:domain moving_measure:Measure ;
                             rdfs:range moving_measure:MeasuredData ;
                             owl:propertyChainAxiom ( moving_measure:inTheContextOf
                                                      moving_measure:collect
                                                    ) .


###  http://www.alessandrolucco.it/ontologies/moving_measure#haveDataCollectedWith
moving_measure:haveDataCollectedWith rdf:type owl:ObjectProperty ;
                                     owl:propertyChainAxiom ( moving_measure:inTheContextOf
                                                              prov:used
                                                            ) .


###  http://www.alessandrolucco.it/ontologies/moving_measure#inTheAreaOf
moving_measure:inTheAreaOf rdf:type owl:ObjectProperty ;
                           rdfs:domain moving_measure:Measure ;
                           rdfs:range moving_measure:Area ;
                           rdfs:comment "Lega un entità misura a un area geografica" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#inTheContextOf
moving_measure:inTheContextOf rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf prov:wasInfluencedBy ;
                              owl:inverseOf moving_measure:produce ;
                              rdf:type owl:FunctionalProperty ;
                              rdfs:domain moving_measure:Measure ;
                              rdfs:range moving_measure:MeasurementAct ;
                              rdfs:comment "Proprietà più specifica per indicare la relazione tra la misura è l'attività di misurazione" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#obtainedFrom
moving_measure:obtainedFrom rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf prov:wasDerivedFrom ;
                            rdfs:domain moving_measure:MeasuredData ;
                            rdfs:range moving_measure:Equipment ;
                            rdfs:comment "Usata per idicare la propietà di ottenere un certo tipo di dati dalla strumentazione usata" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#produce
moving_measure:produce rdf:type owl:ObjectProperty ;
                       rdfs:comment "Proprietà usata per indicare che l'attività di misura produce una misura" .


###  http://www.w3.org/ns/prov#hadMember
prov:hadMember rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf prov:wasInfluencedBy ;
               rdfs:domain prov:Collection ;
               rdfs:range prov:Entity ;
               rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
               rdfs:label "hadMember" ;
               prov:category "expanded" ;
               prov:component "expanded" ;
               prov:inverse "wasMemberOf" ;
               prov:sharesDefinitionWith prov:Collection .

[ rdf:type owl:Axiom ;
   owl:annotatedSource prov:hadMember ;
   owl:annotatedProperty rdfs:range ;
   owl:annotatedTarget prov:Entity ;
   rdfs:comment "A collection is an entity that provides a structure to some constituents, which are themselves entities. These constituents are said to be member of the collections."@en ;
   prov:dm "http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-collection"
 ] .


###  http://www.w3.org/ns/prov#hadRole
prov:hadRole rdf:type owl:ObjectProperty ;
             rdfs:domain prov:Person ;
             rdfs:range prov:Role ;
             rdfs:comment "Indica il ruolo che ha una persona (ad esempio nell'ambito di un'attività)" ;
             rdfs:label "hadRole" ;
             prov:sharesDefinitionWith prov:Role .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf prov:wasInfluencedBy ;
          rdfs:domain prov:Activity ;
          rdfs:range prov:Entity ;
          rdfs:comment "A prov:Entity that was used by this prov:Activity. For example, :baking prov:used :spoon, :egg, :oven ."@en ;
          rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
          rdfs:label "used" ;
          prov:category "starting-point" ;
          prov:component "entities-activities" ;
          prov:inverse "wasUsedBy" .


###  http://www.w3.org/ns/prov#wasAssociatedWith
prov:wasAssociatedWith rdf:type owl:ObjectProperty ;
                       rdfs:comment "usata per indicare l'associazione tra l'attività e l'agente, o anche tra il l'attività e un certo ruolo ricorperto da un agente" ;
                       rdfs:isDefinedBy "http://www.w3.org/ns/prov#" ;
                       rdfs:label "wasAssociatedWith" .


###  http://www.w3.org/ns/prov#wasAttributedTo
prov:wasAttributedTo rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf prov:wasInfluencedBy ;
                     rdfs:domain prov:Entity ;
                     rdfs:range prov:Agent ;
                     rdfs:comment "Attribution is the ascribing of an entity to an agent."@en ;
                     rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                     rdfs:label "wasAttributedTo" ;
                     prov:category "starting-point" ;
                     prov:component "agents-responsibility" ;
                     prov:definition "Attribution is the ascribing of an entity to an agent."@en ;
                     prov:inverse "contributed" .

[ rdf:type owl:Axiom ;
   owl:annotatedSource prov:wasAttributedTo ;
   owl:annotatedProperty rdfs:subPropertyOf ;
   owl:annotatedTarget prov:wasInfluencedBy ;
   rdfs:comment "Attribution is a particular case of trace (see http://www.w3.org/TR/prov-dm/#concept-trace), in the sense that it links an entity to the agent that ascribed it." ;
   prov:definition "IF wasAttributedTo(e2,ag1,aAttr) holds, THEN wasInfluencedBy(e2,ag1) also holds. "
 ] .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:ObjectProperty ;
                    rdfs:comment "Indica il processo di derivare un entità da un'altra senza passare da un'attività" ;
                    rdfs:isDefinedBy "http://www.w3.org/ns/prov#" ;
                    rdfs:label "wasDerivedFrom"@en .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf prov:wasInfluencedBy ;
                    rdfs:domain prov:Entity ;
                    rdfs:range prov:Activity ;
                    rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                    rdfs:label "wasGeneratedBy" ;
                    prov:category "starting-point" ;
                    prov:component "entities-activities" ;
                    prov:inverse "generated" .


###  http://www.w3.org/ns/prov#wasInfluencedBy
prov:wasInfluencedBy rdf:type owl:ObjectProperty ;
                     rdfs:domain _:genid7 ;
                     rdfs:range _:genid12 ;
                     rdfs:comment "Because prov:wasInfluencedBy is a broad relation, its more specific subproperties (e.g. prov:wasInformedBy, prov:actedOnBehalfOf, prov:wasEndedBy, etc.) should be used when applicable."@en ,
                                  "This property has multiple RDFS domains to suit multiple OWL Profiles. See <a href=\"#owl-profile\">PROV-O OWL Profile</a>." ;
                     rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                     rdfs:label "wasInfluencedBy" ;
                     prov:category "qualified" ;
                     prov:component "agents-responsibility" ;
                     prov:editorialNote """The sub-properties of prov:wasInfluencedBy can be elaborated in more detail using the Qualification Pattern. For example, the binary relation :baking prov:used :spoon can be qualified by asserting :baking prov:qualifiedUsage [ a prov:Usage; prov:entity :spoon; prov:atLocation :kitchen ] .

Subproperties of prov:wasInfluencedBy may also be asserted directly without being qualified.

prov:wasInfluencedBy should not be used without also using one of its subproperties. 
"""@en ;
                     prov:inverse "influenced" .

_:genid7 rdf:type owl:Class ;
          owl:unionOf ( prov:Activity
                        prov:Agent
                        prov:Entity
                      ) .

_:genid12 rdf:type owl:Class ;
           owl:unionOf ( prov:Activity
                         prov:Agent
                         prov:Entity
                       ) .

[ rdf:type owl:Axiom ;
   owl:annotatedSource prov:wasInfluencedBy ;
   owl:annotatedProperty rdfs:domain ;
   owl:annotatedTarget _:genid7 ;
   prov:definition "influencee: an identifier (o2) for an entity, activity, or agent; " ;
   prov:dm "http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-influence"
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource prov:wasInfluencedBy ;
   owl:annotatedProperty rdfs:range ;
   owl:annotatedTarget _:genid12 ;
   prov:definition "influencer: an identifier (o1) for an ancestor entity, activity, or agent that the former depends on;" ;
   prov:dm "http://www.w3.org/TR/2013/REC-prov-dm-20130430/#term-influence"
 ] .


#################################################################
#    Data properties
#################################################################

###  http://www.alessandrolucco.it/ontologies/moving_measure#gpsFilePath
moving_measure:gpsFilePath rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf owl:topDataProperty ;
                           rdfs:domain moving_measure:PathGPS ;
                           rdfs:range xsd:anyURI .


###  http://www.alessandrolucco.it/ontologies/moving_measure#maxSpeed
moving_measure:maxSpeed rdf:type owl:DatatypeProperty ;
                        rdfs:domain moving_measure:Measure ;
                        rdfs:range xsd:decimal ;
                        rdfs:comment "Contiene la velocità massima raggiunta durante una singola misura" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#measureFilePath
moving_measure:measureFilePath rdf:type owl:DatatypeProperty ;
                               rdfs:domain moving_measure:Measure ;
                               rdfs:range xsd:anyURI .


###  http://www.alessandrolucco.it/ontologies/moving_measure#totalDistance
moving_measure:totalDistance rdf:type owl:DatatypeProperty ;
                             rdfs:domain moving_measure:Measure ;
                             rdfs:range xsd:decimal ;
                             rdfs:comment "Contiene la distanza coperta durante una singola misura" .


###  http://www.w3.org/ns/prov#endedAtTime
prov:endedAtTime rdf:type owl:DatatypeProperty ;
                 rdfs:domain prov:Activity ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "The time at which an activity ended. See also prov:startedAtTime."@en ;
                 rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                 rdfs:label "endedAtTime" ;
                 prov:category "starting-point" ;
                 prov:component "entities-activities" ;
                 prov:editorialNote "It is the intent that the property chain holds: (prov:qualifiedEnd o prov:atTime) rdfs:subPropertyOf prov:endedAtTime."@en ;
                 prov:qualifiedForm prov:atTime .


###  http://www.w3.org/ns/prov#startedAtTime
prov:startedAtTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain prov:Activity ;
                   rdfs:range xsd:dateTime ;
                   rdfs:comment "The time at which an activity started. See also prov:endedAtTime."@en ;
                   rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                   rdfs:label "startedAtTime" ;
                   prov:category "starting-point" ;
                   prov:component "entities-activities" ;
                   prov:editorialNote "It is the intent that the property chain holds: (prov:qualifiedStart o prov:atTime) rdfs:subPropertyOf prov:startedAtTime."@en ;
                   prov:qualifiedForm prov:atTime .


#################################################################
#    Classes
#################################################################

###  http://www.alessandrolucco.it/ontologies/moving_measure#Area
moving_measure:Area rdf:type owl:Class ;
                    rdfs:comment "Area interessata dal percorso della singola misura. Ad esempio la città se tutta all'interno dell'area urbana o la provincia" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#BasebandAnalizer
moving_measure:BasebandAnalizer rdf:type owl:Class ;
                                rdfs:subClassOf moving_measure:Equipment ;
                                rdfs:comment "identifica strumenti che analizzano il sgnale una volta demodulato, quindi ad esempio per l'FM analizza la qualità audio, cioè ciò che il segnale \"trasporta\"" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#Driver
moving_measure:Driver rdf:type owl:Class ;
                      rdfs:subClassOf prov:Role .


###  http://www.alessandrolucco.it/ontologies/moving_measure#Equipment
moving_measure:Equipment rdf:type owl:Class ;
                         rdfs:subClassOf prov:Entity ;
                         rdfs:comment "Quali strumenti di misura sono stati usati" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#InCarMeasure
moving_measure:InCarMeasure rdf:type owl:Class ;
                            rdfs:subClassOf moving_measure:VehicleMeasure ;
                            rdfs:comment "Misure che prevedono e simulano l'utilizzo di un device all'interno di un mezzo in movimento (Auto, mezzo pubblico...)" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#Measure
moving_measure:Measure rdf:type owl:Class ;
                       rdfs:subClassOf prov:Entity ;
                       rdfs:comment "La singola misura, intesa per quest'applicazione come insieme di dati presi lungo un percorso con una posizione gps associata a ogni insieme di dati" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#MeasureCampaign
moving_measure:MeasureCampaign rdf:type owl:Class ;
                               rdfs:subClassOf prov:Collection ;
                               rdfs:comment "Identifica l'insieme delle misure che costituiscono la campagna" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#MeasuredData
moving_measure:MeasuredData rdf:type owl:Class ;
                            owl:equivalentClass [ rdf:type owl:Class ;
                                                  owl:oneOf ( <http://www.co-ode.org/ontologies/ont.owl#EMFLevel>
                                                              <http://www.co-ode.org/ontologies/ont.owl#Speed>
                                                              <http://www.co-ode.org/ontologies/ont.owl#VideoQuality>
                                                            )
                                                ] ;
                            rdfs:subClassOf prov:Entity .


###  http://www.alessandrolucco.it/ontologies/moving_measure#MeasurementAct
moving_measure:MeasurementAct rdf:type owl:Class ;
                              rdfs:subClassOf prov:Activity ;
                              rdfs:comment """Identifica le attività di misura, è un attività che produce una o più entità \"Measure\" seguendo i relativi percorsi GPS. A un individuo di tipo \"MeasurementAct\" possono essere associati tramite le relative proprietà:
-  uno o più operatori (attraverso il ruolo Operator), 
- il mezzo usato, un conduttore del mezzo (attraverso il ruolo Driver), 
- le attrezzature usate (Equipment), 
- i dati misurati (Data) 
- il software usato (SoftwareAgent)"""@it .


###  http://www.alessandrolucco.it/ontologies/moving_measure#Operator
moving_measure:Operator rdf:type owl:Class ;
                        rdfs:subClassOf prov:Role .


###  http://www.alessandrolucco.it/ontologies/moving_measure#PathGPS
moving_measure:PathGPS rdf:type owl:Class ;
                       rdfs:subClassOf prov:Entity .


###  http://www.alessandrolucco.it/ontologies/moving_measure#PedestrianMeasure
moving_measure:PedestrianMeasure rdf:type owl:Class ;
                                 rdfs:subClassOf moving_measure:Measure ;
                                 rdfs:comment "Misure che testano la ricezione del segnale con dispositivi mobili e l'operatore che si muove a piedi (ad eempio in campo urbano)" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#PowerMeasure
moving_measure:PowerMeasure rdf:type owl:Class ;
                            owl:equivalentClass [ rdf:type owl:Restriction ;
                                                  owl:onProperty moving_measure:haveDataCollectedWith ;
                                                  owl:someValuesFrom moving_measure:PowerMeter
                                                ] ;
                            rdfs:subClassOf moving_measure:Measure ;
                            rdfs:comment "Misure che effettuano il log della potenza del segnale ricevuto." .


###  http://www.alessandrolucco.it/ontologies/moving_measure#PowerMeter
moving_measure:PowerMeter rdf:type owl:Class ;
                          rdfs:subClassOf moving_measure:Equipment .


###  http://www.alessandrolucco.it/ontologies/moving_measure#QualityMeasure
moving_measure:QualityMeasure rdf:type owl:Class ;
                              owl:equivalentClass [ rdf:type owl:Restriction ;
                                                    owl:onProperty moving_measure:haveDataCollectedWith ;
                                                    owl:someValuesFrom moving_measure:BasebandAnalizer
                                                  ] ;
                              rdfs:subClassOf moving_measure:Measure ;
                              rdfs:comment "Misure che effettuano il log della qualità del segnale ricevuto (il tipo di dato dipende dal tipo di segnale)" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#TransportMean
moving_measure:TransportMean rdf:type owl:Class ;
                             rdfs:subClassOf prov:Entity ;
                             rdfs:comment "Identifica quale mezzo è stato usato per la misura, ad esempio se in macchina, in bici o a piedi"@it ;
                             rdfs:seeAlso "http://dbpedia.org/resource/Mode_of_transport" ;
                             skos:closeMatch "https://dbpedia.org/ontology/MeanOfTransportation" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#VehicleMeasure
moving_measure:VehicleMeasure rdf:type owl:Class ;
                              rdfs:subClassOf moving_measure:Measure ;
                              rdfs:comment "Misure che prevedono la ricezione del segnale da un antenna posta su tetto del mezzo" .


###  http://www.co-ode.org/ontologies/ont.owl#Smartphone
<http://www.co-ode.org/ontologies/ont.owl#Smartphone> rdf:type owl:Class ;
                                                      rdfs:subClassOf moving_measure:Equipment .


###  http://www.w3.org/ns/prov#Activity
prov:Activity rdf:type owl:Class ;
              owl:disjointWith prov:Entity ;
              rdfs:isDefinedBy "http://www.w3.org/ns/prov-o#" ;
              rdfs:label "Activity" ;
              prov:definition "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities." .


###  http://www.w3.org/ns/prov#Agent
prov:Agent rdf:type owl:Class ;
           rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
           rdfs:label "Agent" ;
           prov:definition "An agent is something that bears some form of responsibility for an activity taking place, for the existence of an entity, or for another agent's activity. "@en .


###  http://www.w3.org/ns/prov#Collection
prov:Collection rdf:type owl:Class ;
                rdfs:subClassOf prov:Entity ;
                rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                rdfs:label "Collection" ;
                prov:definition "A collection is an entity that provides a structure to some constituents, which are themselves entities. These constituents are said to be member of the collections."@en .


###  http://www.w3.org/ns/prov#Entity
prov:Entity rdf:type owl:Class ;
            rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
            rdfs:label "Entity" ;
            prov:definition "An entity is a physical, digital, conceptual, or other kind of thing with some fixed aspects; entities may be real or imaginary. "@en .


###  http://www.w3.org/ns/prov#Organization
prov:Organization rdf:type owl:Class ;
                  rdfs:subClassOf prov:Agent ;
                  rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                  rdfs:label "Organization" ;
                  prov:definition "An organization is a social or legal institution such as a company, society, etc." .


###  http://www.w3.org/ns/prov#Person
prov:Person rdf:type owl:Class ;
            rdfs:subClassOf prov:Agent ;
            rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
            rdfs:label "Person" ;
            prov:definition "Person agents are people."@en .


###  http://www.w3.org/ns/prov#Role
prov:Role rdf:type owl:Class ;
          rdfs:label "Role" ;
          rdfs:seeAlso prov:hadRole ;
          prov:definition "A role is the function of an entity or agent with respect to an activity, in the context of a usage, generation, invalidation, association, start, and end."@en .


###  http://www.w3.org/ns/prov#SoftwareAgent
prov:SoftwareAgent rdf:type owl:Class ;
                   rdfs:subClassOf prov:Agent ;
                   rdfs:isDefinedBy <http://www.w3.org/ns/prov-o#> ;
                   rdfs:label "SoftwareAgent" ;
                   prov:definition "A software agent is running software."@en .


#################################################################
#    Individuals
#################################################################

###  http://www.alessandrolucco.it/ontologies/moving_measure#ALCMeter
moving_measure:ALCMeter rdf:type owl:NamedIndividual ,
                                 prov:SoftwareAgent ;
                        rdfs:comment "Software per la registrazione automatica di misure legate alla posizione GPS" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#AZ123BX
moving_measure:AZ123BX rdf:type owl:NamedIndividual ,
                                moving_measure:TransportMean ;
                       rdfs:comment "Automobile Vito" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#BridgetechProbe
moving_measure:BridgetechProbe rdf:type owl:NamedIndividual ,
                                        moving_measure:BasebandAnalizer ;
                               rdfs:comment "Analizzatore per i segnali broadcast" .


###  http://www.alessandrolucco.it/ontologies/moving_measure#M20240614-P25
moving_measure:M20240614-P25 rdf:type owl:NamedIndividual ,
                                      moving_measure:Measure ;
                             moving_measure:followGPSPath moving_measure:P25-cavalli-pnuova ;
                             moving_measure:inTheContextOf moving_measure:MA20240614 ;
